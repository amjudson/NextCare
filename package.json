    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.19.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "^8.5.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
